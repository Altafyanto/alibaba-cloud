<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springframework.auth.mapper.MemberMapper">
    <resultMap id="userResult" type="com.springframework.auth.domain.po.UserDO">
        <result property="id" column="id" jdbcType="BIGINT"/>
        <result property="createdTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="createdBy" column="create_by" jdbcType="VARCHAR"/>
        <result property="lastModifiedTime" column="last_modified_time" jdbcType="TIMESTAMP"/>
        <result property="lastModifiedBy" column="last_modified_by" jdbcType="VARCHAR"/>
        <result property="userName" column="user_name" jdbcType="VARCHAR"/>
        <result property="passWord" column="pass_word" jdbcType="VARCHAR"/>
        <result property="sex" column="sex" jdbcType="VARCHAR"/>
        <result property="phoneNumber" column="phone_number" jdbcType="VARCHAR"/>
        <result property="email" column="email" jdbcType="VARCHAR"/>
        <result property="status" column="status" jdbcType="VARCHAR"/>
        <result property="deleted" column="is_deleted" jdbcType="INTEGER"/>
        <result property="introduce" column="introduce" jdbcType="VARCHAR"/>
    </resultMap>
</mapper>